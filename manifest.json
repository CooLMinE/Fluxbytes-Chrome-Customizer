{
  "name": "Fluxbytes Chrome Customizer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An extension with various features and options dedicated to improving the user experience when using Google Chrome.",
  "homepage_url": "http://www.fluxbytes.com",
  "icons": {
    "16": "icons/18x.png",
    "48": "icons/46x.png",
    "128":"icons/128x.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "tabs",
    "history",
    "storage",
    "downloads",
    "contextMenus"
  ]
}